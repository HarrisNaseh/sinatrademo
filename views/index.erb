 <!DOCTYPE html>
<html lang= "en">
    <style>
table, th, td {
  border:1px solid black;
  margin: auto;
}

td, h1{
    text-align: center;
}



</style>
  <body>
    <h1>List Of Cakes</h1>
  <div id ="json">
    <table style="width:50%" id="table">
    
  <tr>
    <th>ID</th>
    <th>Cake Name</th>
  </tr>
</table>
  </div>

  </body>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
  <script type = "text/javascript"> 
    $(document).ready(function(){
      $.getJSON("/cakes.json", function (data) {
        $.each(data, function(id, cake) {
            tableEntry = "<tr><td>"+(id+1)+"</td>\n<td>"+cake+"</td>\n</tr>";
            // $("#json").append((id+1)+ ": "+cake+"<br>")
            $("#table").append(tableEntry);
  });
      })


});
  </script>

</html>


